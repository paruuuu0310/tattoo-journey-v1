# Appfile for Tattoo Journey Mobile App

# iOS Configuration
apple_id(ENV["APPLE_ID"] || "your-apple-id@example.com")
app_identifier(ENV["APP_IDENTIFIER"] || "com.karamon.tattoojourney")
team_id(ENV["APPLE_TEAM_ID"] || "YOUR_TEAM_ID")

# Android Configuration
json_key_file(ENV["GOOGLE_PLAY_JSON_KEY_FILE"] || "./android/service-account-key.json")
package_name(ENV["APP_IDENTIFIER"] || "com.karamon.tattoojourney")